{
  "type": "module",
  "name": "hookform-field",
  "description": "Easy manage form fields, ensuring type-safety and strong reusability built with react-hook-form",
  "version": "1.1.1",
  "author": {
    "email": "duongductrong06@gmail.com",
    "name": "Duong Duc Trong",
    "url": "https://github.com/duongductrong"
  },
  "license": "MIT",
  "keywords": [
    "react-hook-form",
    "react",
    "nextjs",
    "manage fields",
    "form"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/duongductrong/hookform-field"
  },
  "scripts": {
    "dev": "concurrently \"pnpm build --watch\" \"pnpm storybook\" \"pnpm test\" ",
    "build": "tsup --onSuccess \"pnpm link:self\"",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"{src,tests}/**/*.+(ts|js|tsx|jsx)\"",
    "lint:fix": "pnpm lint --fix && prettier --write .",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "prepare": "husky",
    "commit": "cz",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint-staged": "lint-staged --allow-empty --config .lintstagedrc.json",
    "release": "pnpm build && pnpm release-it",
    "link:self": "pnpm link --global",
    "publish:local": "pnpm build && yalc publish && yalc push"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.26.0",
    "@ryansonshine/commitizen": "4.2.8",
    "@ryansonshine/cz-conventional-changelog": "3.3.4",
    "@storybook/addon-essentials": "8.4.6",
    "@storybook/addon-interactions": "8.4.6",
    "@storybook/addon-links": "8.4.6",
    "@storybook/addon-webpack5-compiler-swc": "1.0.5",
    "@storybook/blocks": "8.4.6",
    "@storybook/react": "8.4.6",
    "@storybook/react-webpack5": "8.4.6",
    "@storybook/test": "8.4.6",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@types/node": "20.14.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-test-renderer": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "@vitest/coverage-v8": "1.6.0",
    "babel-loader": "9.2.1",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-storybook": "0.11.1",
    "eslint-plugin-typescript-sort-keys": "3.3.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "husky": "9.1.7",
    "jsdom": "24.1.0",
    "lint-staged": "15.2.10",
    "prettier": "3.4.1",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.51.5",
    "react-test-renderer": "18.3.1",
    "release-it": "17.10.0",
    "storybook": "8.4.6",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vitest": "1.6.0"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "resolutions": {},
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "clsx": "^2.1.1"
  }
}
